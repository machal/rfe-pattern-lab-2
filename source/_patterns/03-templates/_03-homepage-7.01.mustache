{{^page-no-header-no-footer}}<!-- if not tru, will display header & footer - USED for Quiz-embedded page -->
{{> molecules-header }}
{{/page-no-header-no-footer}}

<div id="content">
    {{> molecules-section-page-title-wrapper }}
    <div class="container">

        <div class="row">

            <div class="col-md-8 col-lg-8 pull-left">
                <!-- TOP Story -->
                <div class="media-block-wrap">
                    {{> molecules-media-block-holder-header}}
                    <div class="row">
                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                            <div class="row">
                                {{> molecules-media-block-holder(size:2, item-count:"one", class:"col-xs-12 col-sm-12 col-md-12 col-lg-12", showIntro:true, showTitle:true, showImg:true, showCategory:false, showTime:false, showDate:false, showDateRelative:false, showCounter:false,horizontal:false) }}
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 follow-up-list">
                            <div class="row">
                                {{> molecules-media-block-holder(size:4, item-count:"four",  class:"col-xs-12 col-sm-12 col-md-12 col-lg-12", showIntro:false, showTitle:true, showImg:true, showCategory:false, showTime:false, showDate:false, showDateRelative:true, showCounter:false,horizontal:true) }}
                            </div>
                        </div>
                    </div>
                    {{> molecules-media-block-holder-footer}}
                </div>

                <!-- SUB stories -->
                <!--
                <div class="media-block-wrap col-xs-12 col-sm-12 col-md-12">

                    <div class="row">
                        {{!> molecules-media-block-holder(size:4, item-count:"two", class:"col-xs-12 col-sm-6 col-md-6 col-lg-6", showIntro:false, showTitle:true, showImg:true, showCategory:false, showTime:false, showDate:false, showDateRelative:false, showCounter:false,horizontal:true) }}
                    </div>
                </div>
                -->


            </div>

            <!-- Most popular / numbered -->
            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                {{> molecules-most-popular:numbered }}
            </div>

            <!-- 3-Big img -->
            <div class="media-block-wrap col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                <hr />
                <div class="row">
                    {{> molecules-media-block-holder(size:2, item-count:"three", class:"col-xs-12 col-sm-6 col-md-4 col-lg-4", showIntro:false, showTitle:true, showImg:true, showCategory:true, showTime:false, showDate:false, showDateRelative:false, showCounter:false,horizontal:false) }}
                </div>
            </div>


            <!-- two widget sets -->
            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                <!--             2-Big img
                            <div class="media-block-wrap">
                                {{#specific-items-bottom-left }}
                                {{!> molecules-media-block-holder-header}}
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                                    {{!> molecules-media-block-holder-specific(size:2, item-count:"two", class:"col-xs-12 col-sm-4 col-md-6 col-lg-6", showIntro:false, showTitle:true, showImg:true, showCategory:true, showTime:false, showDate:false, showDateRelative:false, showCounter:false,horizontal:false) }}
                                </div>
                                {{/specific-items-bottom-left  }}
                                {{!> molecules-media-block-holder-footer}}
                            </div>-->
                <!-- 2-Thumbs + List -->
                <div class="media-block-wrap">
                    {{> molecules-media-block-holder-header}}
                    <div class="row">
                        <div class="col-sm-6 col-md-6 col-lg-6 col-xs-12">
                            <div class="row">
                                {{> molecules-media-block-holder(size:3, item-count:"two", class:"col-xs-12 col-sm-12 col-md-12 col-lg-12", showIntro:false, showTitle:true, showImg:true, showCategory:true, showTime:false, showDate:false, showDateRelative:false, showCounter:false,horizontal:true) }}
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6 follow-up-list">
                            <div class="row">
                                {{> molecules-media-block-holder(size:4, item-count:"five", class:"col-xs-12 col-sm-12 col-md-12 col-lg-12", showIntro:false, showTitle:true, showImg:false, showCategory:false, showTime:false, showDate:false, showDateRelative:false, showCounter:false,horizontal:true) }}
                            </div>
                        </div>
                    </div>
                    {{> molecules-media-block-holder-footer}}
                </div>
            </div>

            <!-- Contact US -->
            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                {{> molecules-contact-us}}
            </div>

        </div><!-- /.row-->
    </div><!-- /.container-->
    <!-- patternlab-only -->
</div><!-- /#content-->
{{^page-no-header-no-footer}}   <!-- if not tru, will display header & footer - USED for Quiz-embedded page -->
{{> molecules-footer }}
{{/page-no-header-no-footer}}
